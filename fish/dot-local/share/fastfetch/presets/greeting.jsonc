{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "small",
    "padding": {
      "left": 1,
      "top": 1
    }
  },
  "display": {
    "separator": "",
    "key": {
      "width": 3
    },
    "size": {
      "maxPrefix": "MB",
      "ndigits": 0
    }
  },
  "modules": [
    "title",
    {
      "type": "os",
      "key": "󰌢",
      "format": "{name} {version}"
    },
    {
      "type": "kernel",
      "key": "󰞸",
      "format": "{release}"
    },
    {
      "type": "cpu",
      "key": "",
      "temp": true,
      "format": "{name} ({temperature})"
    },
    {
      "type": "memory",
      "key": "",
      "format": "{used}/{total} ({percentage})"
    },
    {
      "type": "gpu",
      "key": "󰍹",
      "temp": true,
      "format": "{name} [{type}] ({temperature})"
    },
    {
      "type": "uptime",
      "key": "󰅶"
    },
    {
      "type": "battery",
      "key": "󱐋"
    },
    {
      "type": "localip",
      "key": "󱂇"
    }
  ],
}
